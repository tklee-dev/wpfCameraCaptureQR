<Window x:Class="CameraCaptureQR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Moki"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1280" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid >
  
        

        <StackPanel Grid.Column="1">
            
            <ComboBox x:Name="cboDevice" />
            <TextBox x:Name="tbQRCode" />
            <Image x:Name="frameHolder" Grid.Column="0"/>
            
        </StackPanel>
        
    </Grid>
</Window>
